{
  "name": "type-safe-ipc",
  "version": "1.0.0",
  "type": "module",
  "main": "src/main/dist/main.cjs",
  "scripts": {
    "start": "npm run build && run-p electron serve",
    "build": "npm run build:main && npm run build:preload",
    "build:main": "cd src/main && vite build",
    "build:preload": "cd src/preload && vite build",
    "electron": "electron .",
    "serve": "cd src/renderer && vite",
    "lint": "run-s lint:main lint:preload lint:renderer",
    "lint:main": "cd src/main && eslint .",
    "lint:preload": "cd src/preload && eslint .",
    "lint:renderer": "cd src/renderer && eslint ."
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^34.3.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-morph": "^25.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0"
  }
